{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:server": "tsc -p tsconfig.server.json",
    "server": "node dist-server/index.js",
    "dev:server": "cross-env NODE_ENV=development tsx --tsconfig ./tsconfig.server.json server/index.ts",
    "simple:server": "tsx --tsconfig ./tsconfig.server.json server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run simple:server\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
